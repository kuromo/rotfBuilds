extends layout

block content
	section#treeTop.row
		#treeTopCont.container
			h1 #{ treeTitle }
			h3 
				p #{ treeDesc }
				span.ptCount #{ points } 
				span /82

	section#treeTree.row
		#treeWrap
			#treeCont.treeSub
				.tileWrap
					each row, rId in tree
						each col, cId in row
							div(class="tile #{ col.type }" data-id="#{ rId+cId }")
								.tileCont 
									p #{ col.type }
									p #{ col.val }
			#tStatCont.treeSub
				h3 small runes
				#smRune
					button.smRune.btn.btn-default
						img.rRuneIcon(src="img/looter.jpg")
					button.smRune.btn.btn-default
						img.rRuneIcon(src="img/looter.jpg")
					button.smRune.btn.btn-default
						img.rRuneIcon(src="img/looter.jpg")
					button.smRune.btn.btn-default
						img.rRuneIcon(src="img/looter.jpg")
					button.smRune.btn.btn-default
						img.rRuneIcon(src="img/looter.jpg")
					button.smRune.btn.btn-default
						img.rRuneIcon(src="img/looter.jpg")
					button.smRune.btn.btn-default
						img.rRuneIcon(src="img/looter.jpg")
					button.smRune.btn.btn-default
						img.rRuneIcon(src="img/looter.jpg")
				h3 big runes
				#bigRunes
					.dropdown.runeSel
						button.btn.btn-default.dropdown-toggle(type="button" data-toggle="collapse" data-target="#runeRedDD" aria-controls="navbarDropdown" aria-expanded="false")
							img.rRuneIcon(src="img/looter.jpg")
						#runeRedDD.dropdown-menu(role="menu" aria-labelledby="classDDBtn")
							.runeDDItem(role="menuitem" onClick="") 
								img(src="img/looter.jpg") 
								span Looter
							.runeDDItem(role="menuitem" onClick="") 
								img(src="img/looter.jpg") 
								span Other R
					.dropdown.runeSel
						button.btn.btn-default.dropdown-toggle(type="button" data-toggle="collapse" data-target="#runeGreenDD" aria-controls="navbarDropdown" aria-expanded="false")
							img.gRuneIcon(src="img/looter.jpg")
						#runeGreenDD.dropdown-menu(role="menu" aria-labelledby="classDDBtn")
							.runeDDItem(role="menuitem" onClick="") 
