								span Looter
							.runeDDItem(role="menuitem" onClick="") 
								img(src="img/looter.jpg") 
								span Other G
					.dropdown.runeSel
						button.btn.btn-default.dropdown-toggle(type="button" data-toggle="collapse" data-target="#runeBlueDD" aria-controls="navbarDropdown" aria-expanded="false")
							img.bRuneIcon(src="img/looter.jpg")
